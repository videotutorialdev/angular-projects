/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "@components/button": [
        "dist/components/button/components-button",
        "dist/components/button"
      ],
      "@components/card": [
        "dist/components/card/components-card",
        "dist/components/card"
      ],
      "@components/forms": [
        "dist/components/forms/components-forms",
        "dist/components/forms"
      ],
      "@components/popup": [
        "dist/components/popup/components-popup",
        "dist/components/popup"
      ],
      "utils": [
        "dist/utils/utils",
        "dist/utils"
      ],
      "validators": [
        "dist/validators/validators",
        "dist/validators"
      ]
    },
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false
  }
}
